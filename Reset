    ' Step 10: Populate Column C with the intended department user
        wsSelectionPage.Range("C2").value = "Accounts Receivable (AR) Team"
        wsSelectionPage.Range("C3").value = "Salesforce Team"
        wsSelectionPage.Range("C4").value = "Accounts Receivable (AR) Team"
