Public Sub Selection_Page_NoPrompt()

''' This macro is meant to go back to selection page (re-creating the selection page) - To be able to select a macro to run. '''

    Dim response As VbMsgBoxResult
    Dim wsSelectionPage As Worksheet
    Dim TimeSheet As String
    Dim ws As Worksheet
    
    ' Variables for Buttons for AR Team
        Dim AR_225Gives As Button
        Dim AR_AZGives As Button
        Dim AR_Benevity As Button
        Dim AR_BigGives As Button
        Dim AR_ClickAndPledge As Button
        Dim AR_CyberGrants As Button
        Dim AR_FidelityGifts As Button
        Dim AR_FrontStream As Button
        Dim AR_NTXGiving As Button
        Dim AR_YourCause As Button
    
    ' Variables for Buttons for SF Team
        Dim SF_225Gives As Button
        Dim SF_AZGives As Button
        Dim SF_Benevity As Button
        Dim SF_BigGives As Button
        Dim SF_ClickAndPledge As Button
        Dim SF_CyberGrants As Button
        Dim SF_FidelityGifts As Button
        Dim SF_FrontStream As Button
        Dim SF_NTXGiving As Button
        Dim SF_YourCause As Button
    
    ' Links Variables
        Dim SFReport_ManualImports As String
        Dim SFReport_ClickandPledge As String
        
        SFReport_ManualImports = "https://basised.lightning.force.com/lightning/r/Report/00OHp000008pMl6MAE/view?queryScope=userFolders"
        SFReport_ClickandPledge = "https://basised.lightning.force.com/lightning/r/Report/00ORj000001es7ZMAQ/view?queryScope=userFolders"
    
    Application.Calculation = xlManual

    
    
    ' Step 3: Create a variable called TimeSheet to hold the value "ZYXWV_" and the current date-time (YYYY.MM.DD-HH.MM.SS) - Year.Month.Day-Hour.Minute.Second _
        ' An example of this would look like "ZYXWV_2024.05.06-13.47.06"
            TimeSheet = "ZYXWV_" & Format(Now(), "YYYY.MM.DD-HH.MM.SS")
    
    
    ' Step 4: Use the TimeSheet value to create a new worksheet (one that is very unlikely to already have that name)
        Set wsSelectionPage = ThisWorkbook.Worksheets.Add
        wsSelectionPage.Name = TimeSheet
        
        
    ' Step 5: Keep only the worksheet with the name with the variable TimeSheet
        ' Step 5.1: To speed up the process, don't display alerts
            Application.DisplayAlerts = False
        ' Step 5.2: Create the loop to only keep the Worksheet with the variable TimeSheet Name
            For Each ws In ThisWorkbook.Worksheets
                If ws.Name <> TimeSheet Then
                    ws.Delete
                End If
            Next ws
        ' Step 5.3: Turn back on displaying alerts
            Application.DisplayAlerts = True


    ' Step 6: Rename TimeSheet Worksheet to "Converter Selection Page"
        wsSelectionPage.Name = "Converter Selection Page"

    
    ' Step 7: Create the headers for the "Converter Selection Page" Worksheet.
        wsSelectionPage.Range("A1").value = "Press Button Below To Run Macro"
        wsSelectionPage.Range("B1").value = "Converter Name"
        wsSelectionPage.Range("C1").value = "Converter's Department"
        wsSelectionPage.Range("D1").value = "Donation Website Link"
        wsSelectionPage.Range("E1").value = "Salesforce Report Link"
        wsSelectionPage.Range("F1").value = "Converter's Description"
        
    
    ' Step 8: Format the Column Headers' Row
        With wsSelectionPage.Range("A1:F1")
            .Font.Bold = True
            .Font.Underline = True
            .AutoFilter
        End With
        
    
    ' Step 9: Populate Column B with the Converter Names
        wsSelectionPage.Range("B2").value = "225 Gives"
        wsSelectionPage.Range("B3").value = "225 Gives"
        wsSelectionPage.Range("B4").value = "AZ Gives"
        wsSelectionPage.Range("B5").value = "AZ Gives"
        wsSelectionPage.Range("B6").value = "Benevity"
        wsSelectionPage.Range("B7").value = "Benevity"
        wsSelectionPage.Range("B8").value = "Big Gives/Bonterra Tech/Give Gab"
        wsSelectionPage.Range("B9").value = "Big Gives/Bonterra Tech/Give Gab"
        wsSelectionPage.Range("B10").value = "Click & Pledge"
        wsSelectionPage.Range("B11").value = "Click & Pledge"
        wsSelectionPage.Range("B12").value = "Cyber Grants"
        wsSelectionPage.Range("B13").value = "Cyber Grants"
        wsSelectionPage.Range("B14").value = "Fidelity Gifts"
        wsSelectionPage.Range("B15").value = "Fidelity Gifts"
