            ' if the CRJNeg isn't empty then fill in the data, otherwise leave it all blank.
                If wsCRJNeg.Range("B2").value <> "" Then
                    ' Steps : Populate the: wsCHARNegStart ("Neg. CHAR - Start" worksheet)
                        ' Step : Create the columns headers.
