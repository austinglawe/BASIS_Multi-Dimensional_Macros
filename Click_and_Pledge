                    wsAccountDesignation.Range("A20001").Formula2 = "=FILTER('Positive SF Data'!B:B,'Positive SF Data'!AH:AH=""CHECK"","""")"
                    wsAccountDesignation.Range("B20001").Formula = "=IF(A20001<>"""",XLOOKUP(A20001,'SF Filtered'!A:A,'SF Filtered'!D:D),"""")"
